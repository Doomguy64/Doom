+// Enum_NumForName
+//
+// Returns the value of an enum name, or -1 if not found.
+//
+//==========================================================================
+
+static int Enum_NumForName(const FEnumList *list, const char *name)
+{
+	while (list->Name != NULL)
+	{
+		if (stricmp(list->Name, name) == 0)
+		{
+			return list->Value;
+		}
+		list++;
+	}
+	return -1;
