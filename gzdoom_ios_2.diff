+void buttonChange(int state,FButtonStatus *button)
+{
+	if (state)
+	{
+		button->bDown = true;
+		button->bWentDown  = true;
+	}
+	else
+	{
+		button->bWentUp = true;
+		button->bDown = false;
+	}
